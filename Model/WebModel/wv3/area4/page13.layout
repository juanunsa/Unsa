<wr-page>
  <wr-content for="logo">
    <img src="assets/WebDefault/images/webratio-logo.png" class="wd-logo-top-left">
  </wr-content>
  <wr-content for="login">
    <wd-alert [messages]="valueOf('main/msg8/messages')" [localized]="true" type="primary"></wd-alert>
    <wd-alert [messages]="valueOf('main/msg7/messages')" type="warn" [localized]="true" [dismissable]="true"></wd-alert>
    <wd-card title="main/frm6">
      <wr-content for="content">
        <form>
	        <wd-password 
	            context="main/frm6/fld16" 
	            floatLabel="never" 
	            >
	        </wd-password>
	        <wd-password 
	            context="main/frm6/fld17" 
	            floatLabel="never" 
	            >
	        </wd-password>
	        <wd-password 
	            context="main/frm6/fld18" 
	            floatLabel="never" 
	            >
	        </wd-password>
			<wd-button 
			    context="main/frm6/oslte15" 
			    color="primary"
			    >
			</wd-button>
	    </form>
      </wr-content>
      <wr-content for="title[frm6]">
        {{ labelOf('.') | wrLocalize }}
      </wr-content>
    </wd-card>
  </wr-content>
</wr-page>